<launch>
	<include file="$(find warthog_mapping)/launch/rslidar_32.launch"/>

	<node pkg="norlab_icp_mapper" type="mapper_node" name="mapper_node">
		<remap from="points_in" to="rslidar_points"/>
		<param name="sensor_frame" value="rslidar"/>
		<param name="robot_frame" value="rslidar"/>
		<param name="odom_frame" value="rslidar"/>
		<param name="input_filters_config" value="$(find warthog_mapping)/params/realtime_input_filters.yaml"/>
		<param name="map_update_condition" value="delay"/>
		<param name="map_update_delay" value="0"/>
		<param name="sensor_max_range" value="10"/>
	</node>
</launch>
